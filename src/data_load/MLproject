name: data_load

conda_env: conda.yml

entry_points:
  main:
    parameters:
      data_load.data_stage: {type: str, default: raw}
      data_load.output_dir: {type: str, default: artifacts}
    command: >
      python run.py data_load.data_stage={data_load.data_stage} data_load.output_dir={data_load.output_dir}
